          <li><a href="facebook.com"><i class="icon ion-social-googleplus"> </i>GOOGLE</a></li>
        </ul>
      </div>
      <div class="col l3">
        <ul>
          <li><a href="/faq"><i class="icon ion-help-circled"> </i>FAQs</a></li>
          <li><a href="/terms"><i class="icon ion-document-text"> </i>TERMS & CONDITIONS</a></li>
